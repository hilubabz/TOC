#include<iostream>
#include<string>
using namespace std;

int main(){
    string a;
    int i,l,state=0,s;
    cout<<"Enter a string :";
    cin>>a;
    l=a.length();
    for(i=0;i<l;i++){
        if(a[i]=='a'||a[i]=='b'){
            cout<<"Invalid String";
            return 0;
        }
    }
    
    for(i=0;i<l;i++){
        if(state==0 && a[i]=='0')
            state=1;
        else if(state==0 && a[i]=='1')
            state=3;
        else if(state==1 && a[i]=='0')
            state=3;
        else if(state==1 && a[i]=='1')
            state=2;
        else if(state==2 && a[i]=='0')
            state=2;
        else if(state==2 && a[i]=='1')
            state=2;
        else if(state==3 && a[i]=='0')
            state=3;
        else if(state==3 && a[i]=='1')
            state=3;
    }
    
    if(state==2){
        cout<<"String is accepted";
    }
    else{
        cout<<"String is not accepted";
    }
    return 0;
}
