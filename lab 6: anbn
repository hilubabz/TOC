#include<iostream>
using namespace std;


int main(){
    string A;
    int i=0;
    cout<<"Enter a string :";
    cin>>A;
    
    while(true){
        if(A[i]=='Y'){
            for(int j=i+1;j<A.length();j++){
                if(A[j]!='Y'){
                    cout<<"String rejected";
                    return 0;
                }
            }
            break;
        }
        if(A[i]=='a'){
            A[i]='X';
            for(int j=i+1;j<=A.length();j++){
                if(j==A.length()){
                    cout<<"String rejected";
                    return 0;
                }
                if(A[j]=='b'){
                    A[j]='Y';
                    break;
                }
            }
            
        }
        else{
            cout<<"String rejected";
            return 0;
        }
        i++;
    }
    cout<<"String accepted";
}
